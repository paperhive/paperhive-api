    },
    "title": {
      "id": "http://jsonschema.net/title#",
      "type": "string",
      "default": "",
      "additionalProperties": false
    },
    "date": {
      "id": "http://jsonschema.net/date#",
      "type": "string",
      "default": "publication date, 2014-10-15T16:31:32.040Z",
