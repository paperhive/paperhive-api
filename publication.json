{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://jsonschema.net#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "origins": {
      "id": "http://jsonschema.net/origins#",
      "type": "array",
      "additionalProperties": false,
      "items": {
        "id": "http://jsonschema.net/origins/0#",
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "url": {
            "id": "http://jsonschema.net/origins/0/url#",
            "type": "string",
            "default": "http://arxiv.org/wurstwasser",
            "additionalProperties": false
          },
          "type": {
            "id": "http://jsonschema.net/origins/0/type#",
            "type": "string",
            "default": "arxiv",
            "additionalProperties": false
          },
          "date_added": {
            "id": "http://jsonschema.net/origins/0/date_added#",
            "type": "string",
            "default": "2014-10-15T16:31:32.040Z",
            "additionalProperties": false
          },
          "added_by": {
            "id": "http://jsonschema.net/origins/0/added_by#",
            "type": "string",
            "default": "user id",
            "additionalProperties": false
          }
        },
        "required": [
          "url",
          "type",
          "date_added",
          "added_by"
        ]
      },
      "required": [
        "0"
      ]
    },
    "md5sum": {
      "id": "http://jsonschema.net/md5sum#",
      "type": "string",
      "default": "MD5 Checksum of the PDF",
      "additionalProperties": false
    },
    "authors": {
      "id": "http://jsonschema.net/authors#",
      "type": "array",
      "additionalProperties": false,
      "items": [
        {
          "id": "http://jsonschema.net/authors/0#",
          "type": "string",
          "default": "person_d1",
          "additionalProperties": false
